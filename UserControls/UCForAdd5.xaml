<UserControl x:Class="Auto_Runer.UCForAdd5"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                xmlns:local="clr-namespace:Auto_Runer"
              mc:Ignorable="d" Height="113.5" Width="280">
    <Grid Margin="0,0,10,0">
        <Label Content="Цикличная задача" Margin="10,54,123,39"></Label>
        <Label Content="Обычная задача" Margin="10,75,123,18"></Label>
        <Label Content="Выберите тип задачи" Margin="71,10,62,83"></Label>
        <CheckBox x:Name="CheckBox_normal" Margin="155,80,99,17" RenderTransformOrigin="0.5,0.5" Checked="CheckBox_normal_OnChecked" Unchecked="CheckBox_normal_OnUnchecked">
            <CheckBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.327"/>
                    <TranslateTransform/>
                </TransformGroup>
            </CheckBox.RenderTransform>
        </CheckBox>
        <CheckBox x:Name="CheckBox_cil" Margin="155,55,99,39" Checked="CheckBox_cil_OnChecked" Unchecked="CheckBox_cil_OnUnchecked"></CheckBox>
    </Grid>
